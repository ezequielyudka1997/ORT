 	tabstat porcentajepromenviadoj2, by(edad3349) stat(mean sd)
 	tabstat porcentajepromenviadoj2, by(edad50) stat(mean sd)
		tabstat ret1 ret2 ret3 ret4 ret5, by(edad1832) stat(mean sd N)	

	
	gen ret1= ((porcentajepromenviadoj2_11 +  porcentajepromenviadoj2_21 + porcentajepromenviadoj2_31 + porcentajepromenviadoj2_41)/4)
 	gen ret2= ((porcentajepromenviadoj2_12 +  porcentajepromenviadoj2_22 + porcentajepromenviadoj2_32 + porcentajepromenviadoj2_42)/4)
	gen ret3= ((porcentajepromenviadoj2_13 + porcentajepromenviadoj2_23 + porcentajepromenviadoj2_33 + porcentajepromenviadoj2_43)/4)
	gen ret4= ((porcentajepromenviadoj2_14 + porcentajepromenviadoj2_24 + porcentajepromenviadoj2_34 + porcentajepromenviadoj2_44)/4)
	gen ret5= ((porcentajepromenviadoj2_15 + porcentajepromenviadoj2_25 + porcentajepromenviadoj2_35 + porcentajepromenviadoj2_45)/4)
	tabstat ret1 ret2 ret3 ret4 ret5, by(genero) stat(mean sd N)	
